{
  "name": "declare-render",
  "version": "1.0.13",
  "description": "You can declare canvas shapes by JSON format.",
  "main": "./src/node.ts",
  "module": "./src/browser.ts",
  "browser": "./src/browser.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "node": "./src/node.ts",
      "browser": "./src/browser.ts",
      "default": "./src/node.ts"
    },
    "./node": "./src/node.ts",
    "./browser": "./src/browser.ts"
  },
  "scripts": {
    "rebuild:canvas": "pnpm rebuild canvas",
    "test": "tsx test/test.ts",
    "test:browser": "cd test && vite --config ./vite.config.ts",
    "test:browser:build": "cd test && vite build --config ./vite.config.ts",
    "publish": "pnpm publish --registry=https://registry.npmjs.org"
  },
  "keywords": [
    "JSON",
    "Canvas",
    "Renderer"
  ],
  "author": "Joey",
  "license": "ISC",
  "packageManager": "pnpm@10.18.1",
  "dependencies": {
    "canvas": "^3.2.1",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.19.30",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.4.21"
  },
  "type": "module",
  "peerDependencies": {
    "canvas": "^3.2.1"
  },
  "peerDependenciesMeta": {
    "canvas": {
      "optional": true
    }
  }
}
